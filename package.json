{
  "name": "Agora",
  "version": "0.1.0",
  "description": "Make web social again",
  "main": "src/js/main.js",
  "scripts": {
    "watch:content": "parcel watch js/content.js -d build/ -o content.js",
    "watch:background": "parcel watch js/background.js -d build/ -o background.js",
    "watch": "concurrently \"npm run watch:content\" \"npm run watch:background\"",
    "delete": "rm -rf node_modules && rm package-lock.json"
  },
  "author": "Jorge Fuentes",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "moment": "^2.24.0",
    "parcel-bundler": "^1.12.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "concurrently": "^4.1.0"
  }
}
